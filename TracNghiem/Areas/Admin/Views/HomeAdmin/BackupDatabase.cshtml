
@{
    ViewBag.Title = "BackupDatabase";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPage1.cshtml";
}

@section naviheader{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="~/Admin/HomeAdmin" class="nav-link">Trang chủ</a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="~/admin/HomeAdmin/Backupdatabase" class="nav-link">Sao lưu_phục hồi</a>
        </li>
    </ul>
}

<!-- Tiêu đề nội dung (Header của trang) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
              
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="~/Admin/HomeAdmin">Trang chủ</a></li>
                    <li class="breadcrumb-item active">Sao lưu_phục hồi</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

@if (!string.IsNullOrEmpty(ViewBag.Message))
{
    <div class="alert alert-success">
        @ViewBag.Message
    </div>
}
@if (ViewBag.Error != null)
{
    <p class="text-danger">@ViewBag.Error</p>
}

<div style="text-align:center">
    @Html.ActionLink("Về trang chủ", "Index", "HomeAdmin", null, new { @class = "btn btn-primary" })
</div>
